# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'APIService' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # 网络+数据解析
	pod 'SwiftyJSON'    # Json解析
  pod 'ObjectMapper'  # Json转对象
  pod 'Alamofire'
  pod 'Moya/RxSwift'
  
  # Rx
	pod 'RxSwift'
  pod 'RxCocoa'
  
  # 加解密
  pod 'CryptoSwift'
  
  # 钥匙串
  pod 'KeychainAccess'
  
  # 工具
  pod 'SwiftDate'

end
